{
  "name": "llabbasmkhll/laravel-zibal",
  "description": "transaction request library for zibal",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "abbas mkhzomi",
      "email": "llabbasmkhll@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": "^7.1.3|8.0.*",
    "laravel/framework": "5.*|6.*|7.*|8.*|9.*"
  },
  "autoload": {
    "psr-4": {
